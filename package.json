{
  "name": "cursor-align",
  "displayName": "Cursor Align",
  "description": "Aligns all cursors using spaces.",
  "icon": "img/icon.png",
  "version": "1.2.0",
  "publisher": "huntertran",
  "engines": {
    "vscode": "^1.75.0"
  },
  "license": "MIT",
  "categories": [
    "Formatters"
  ],
  "main": "./extension.js",
  "browser": "./extension.js",
  "extensionKind": [
    "ui",
    "workspace"
  ],
  "contributes": {
    "commands": [
      {
        "command": "huntertran.cursor-align.alignCursors",
        "title": "Align Cursors"
      }
    ],
    "keybindings": [
      {
        "command": "huntertran.cursor-align.alignCursors",
        "key": "alt+a",
        "when": "editorTextFocus"
      }
    ],
    "configuration": [
      {
        "title": "Cursor Align",
        "properties": {
          "align-cursor.alignCharacter": {
            "type": "string",
            "default": "default",
            "enum": [
              "default",
              "tab",
              "space"
            ],
            "enumDescriptions": [
              "Use VSCode default tab character (Recommended)",
              "Use tab characters",
              "Use space characters"
            ]
          }
        }
      }
    ]
  },
  "devDependencies": {
    "eslint": "^8.36.0"
  },
  "bugs": {
    "url": "https://github.com/huntertran/vscode-cursor-align/issues",
    "email": "mike@yo1.dog"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/huntertran/vscode-cursor-align.git"
  },
  "homepage": "https://github.com/huntertran/vscode-cursor-align"
}